<UserControl x:Class="Mov.Standard.Nico.Workspaces.NicoWorkspaceWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mov.Standard.Nico.Workspaces"
             xmlns:behavior="clr-namespace:My.Wpf.Behaviors;assembly=My.Wpf"
             behavior:FrameworkElementBehavior.Loaded="{Binding OnLoaded}"
             mc:Ignorable="d">
    <UserControl.Resources>

        <DataTemplate DataType="{x:Type local:NicoRankingViewModel}">
            <local:NicoRankingWindow />
        </DataTemplate>

        <DataTemplate DataType="{x:Type local:NicoTemporaryViewModel}">
            <local:NicoTemporaryWindow />
        </DataTemplate>

        <DataTemplate DataType="{x:Type local:NicoMylistViewModel}">
            <local:NicoMylistWindow />
        </DataTemplate>

        <DataTemplate DataType="{x:Type local:NicoFavoriteViewModel}">
            <local:NicoFavoriteWindow />
        </DataTemplate>

        <DataTemplate DataType="{x:Type local:NicoVideoHistoryViewModel}">
            <local:NicoVideoHistoryWindow />
        </DataTemplate>

    </UserControl.Resources>

    <Grid>
        <ContentPresenter Content="{Binding}" />

    </Grid>
</UserControl>
