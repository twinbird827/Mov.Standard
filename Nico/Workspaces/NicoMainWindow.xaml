<UserControl x:Class="Mov.Standard.Nico.Workspaces.NicoMainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mov.Standard.Nico.Workspaces"
             mc:Ignorable="d">
    <UserControl.Resources>
        <DataTemplate DataType="{x:Type local:NicoWorkspaceViewModel}">
            <local:NicoWorkspaceWindow />
        </DataTemplate>

    </UserControl.Resources>
    <Grid>
        <ContentPresenter Margin="3"
                              Content="{Binding Current}" />


    </Grid>
</UserControl>
